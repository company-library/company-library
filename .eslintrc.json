{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "indent": ["error", 2],
    "semi": ["error", "never", {"beforeStatementContinuationChars": "never"}],
    "quotes": ["error", "single"],
    "jsx-quotes": ["error", "prefer-double"]
  }
}
