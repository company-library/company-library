    "next": "13.4.9",
    "next-auth": "^4.22.1",
    "prisma": "^4.16.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^1.3.0",
    "urql": "^2.2.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/schema-ast": "^2.4.1",
