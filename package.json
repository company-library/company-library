{
  "name": "company-library",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "biome check src/ test/",
    "lint:fix": "biome check src/ test/ --write",
    "lint:ci": "biome ci src/ test/ --reporter=github",
    "test": "vitest",
    "typeCheck": "tsc --pretty --noEmit",
    "db:generate": "npx prisma generate",
    "db:push": "dotenv -e .env.development.local -- npx prisma db push",
    "db:console": "dotenv -e .env.development.local -- npx prisma studio",
    "db:seed": "dotenv -e .env.development.local -- npx prisma db seed"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@slack/webhook": "^7.0.5",
    "@vercel/blob": "^1.1.1",
    "@vercel/mcp-adapter": "^1.0.0",
    "graphql": "^16.11.0",
    "luxon": "^3.7.1",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "prisma": "^6.13.0",
    "react": "canary",
    "react-dom": "canary",
    "swr": "^2.3.5",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.17.1",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "daisyui": "^5.0.50",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.33.0",
    "eslint-config-next": "15.4.6",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "volta": {
    "node": "22.18.0",
    "yarn": "4.9.2"
  }
}
