{
  "name": "company-library",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "biome check src/ test/",
    "lint:fix": "biome check src/ test/ --write",
    "lint:ci": "biome ci src/ test/ --reporter=github",
    "test": "vitest",
    "typeCheck": "tsc --pretty --noEmit",
    "db:generate": "npx prisma generate",
    "db:push": "dotenv -e .env.development.local -- npx prisma db push",
    "db:console": "dotenv -e .env.development.local -- npx prisma studio",
    "db:seed": "dotenv -e .env.development.local -- npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "@slack/webhook": "^7.0.4",
    "@vercel/blob": "^0.27.0",
    "graphql": "^16.10.0",
    "luxon": "^3.5.0",
    "next": "15.1.2",
    "next-auth": "^4.24.11",
    "prisma": "^6.2.1",
    "react": "canary",
    "react-dom": "canary",
    "swr": "^2.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/eslintrc": "^3.2.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.5",
    "@types/react": "19.0.4",
    "@types/react-dom": "19.0.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.18.0",
    "eslint-config-next": "15.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "vitest-mock-extended": "^2.0.2"
  },
  "volta": {
    "node": "22.13.0",
    "yarn": "4.6.0"
  }
}
