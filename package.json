{
  "name": "company-library",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run db:generate",
    "build": "next build",
    "start": "next start",
    "lint": "biome check src/ test/",
    "lint:fix": "biome check src/ test/ --write",
    "lint:ci": "biome ci src/ test/ --reporter=github",
    "test": "vitest",
    "typeCheck": "tsc --pretty --noEmit",
    "db:generate": "dotenv -e .env.development.local -- npx prisma generate",
    "db:push": "dotenv -e .env.development.local -- npx prisma db push",
    "db:console": "dotenv -e .env.development.local -- npx prisma studio",
    "db:seed": "dotenv -e .env.development.local -- npx prisma db seed"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.2.0",
    "@slack/webhook": "^7.0.6",
    "@vercel/blob": "^2.0.0",
    "graphql": "^16.12.0",
    "luxon": "^3.7.2",
    "next": "15.5.9",
    "next-auth": "^4.24.13",
    "prisma": "^7.2.0",
    "react": "canary",
    "react-dom": "canary",
    "swr": "^2.3.8",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.19.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "daisyui": "^5.5.14",
    "dotenv-cli": "^11.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.16",
    "vitest-mock-extended": "^3.1.0"
  },
  "volta": {
    "node": "22.21.1",
    "yarn": "4.12.0"
  }
}
