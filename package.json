{
  "name": "company-library",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "biome check src/ test/",
    "lint:fix": "biome check src/ test/ --write",
    "lint:ci": "biome ci src/ test/ --reporter=github",
    "test": "vitest",
    "typeCheck": "tsc --pretty --noEmit",
    "db:generate": "npx prisma generate",
    "db:push": "dotenv -e .env.development.local -- npx prisma db push",
    "db:console": "dotenv -e .env.development.local -- npx prisma studio",
    "db:seed": "dotenv -e .env.development.local -- npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "@slack/webhook": "^7.0.3",
    "@vercel/blob": "^0.25.1",
    "graphql": "^16.9.0",
    "luxon": "^3.5.0",
    "next": "14.2.15",
    "next-auth": "^4.24.10",
    "prisma": "^5.21.1",
    "react": "canary",
    "react-dom": "canary",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.17.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.13",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3",
    "vitest-mock-extended": "^2.0.2"
  },
  "volta": {
    "node": "20.18.0",
    "yarn": "4.5.1"
  }
}
