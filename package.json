{
  "name": "company-library",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "biome check src/ test/",
    "lint:fix": "biome check src/ test/ --write",
    "lint:ci": "biome ci src/ test/",
    "test": "jest",
    "typeCheck": "tsc --pretty --noEmit",
    "db:generate": "npx prisma generate",
    "db:push": "dotenv -e .env.development.local -- npx prisma db push",
    "db:console": "dotenv -e .env.development.local -- npx prisma studio",
    "db:seed": "dotenv -e .env.development.local -- npx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@prisma/client": "^5.16.1",
    "@slack/webhook": "^7.0.2",
    "@vercel/blob": "^0.23.4",
    "graphql": "^16.9.0",
    "luxon": "^3.4.4",
    "next": "14.2.4",
    "next-auth": "^4.24.7",
    "prisma": "^5.16.1",
    "react": "canary",
    "react-dom": "canary",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^18.18.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.12.10",
    "dotenv-cli": "^7.4.2",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-mock-extended": "^3.0.7",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "volta": {
    "node": "18.20.3",
    "yarn": "4.2.2"
  }
}
